@model List<Product>;
@{
    ViewBag.Title = "Home Page";
}
@foreach (var item in Model)
{
    <div class="prod-info">
        <img src="~/img/iPhone-Red.png" />
        <ul>
            <li>Price: @item.Price</li>
            <li>Title: @item.Title</li>
            <li>Description: @item.ArtDescription</li>
        </ul>
        <input type="submit" value="Buy item" class="buy" />
    </div>
}


